---
layout: none
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url }}/</loc>
        {% for post in site.posts %}
        {% if forloop.first %}
        <lastmod>{{ post.date | date: "%F" }}</lastmod>
        {% endif %}
        {% endfor %}
        <changefreq>daily</changefreq>
    </url>
</urlset>
